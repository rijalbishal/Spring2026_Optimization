    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x1 = np.linspace(0, 3, 10000)\n",
    "\n",
    "x2_1 = (12.0 - 3.0*x1) / 8.0    \n",
    "x2_2 = 2.0 - x1                  \n",
    "x2_up = np.minimum(x2_1, x2_2)   \n",
    "\n",
    "plt.plot(x1, x2_1, color=\"C0\", label=r'$x_1+\\frac{8}{3}x_2\\leq 4$')\n",
    "plt.plot(x1, x2_2,color=\"C1\", label=r'$x_1+x_2\\leq 2$')\n",
    "plt.axvline(1.5, color=\"C2\", label=r'$2x_1\\leq 3$')  \n",
    "\n",
    "mask = (x1 <= 1.5) & (x2_up >= 0)\n",
    "plt.fill_between(x1, 0, x2_up, where=mask, color='grey', alpha=0.5, interpolate=True)\n",
    "\n",
    "plt.xlim((0, 3))\n",
    "plt.ylim((0, 3))\n",
    "plt.xlabel('$x_1$',fontsize=14)\n",
    "plt.ylabel('$x_2$',fontsize=14)\n",
    "plt.title('Example 3',fontsize=14)\n",
    "plt.grid(True)\n",
    "plt.legend(loc=\"best\",fontsize=14)\n",
    "plt.tight_layout()\n",
    "plt.savefig(\"Example3.png\", dpi=600, bbox_inches=\"tight\", facecolor=\"white\")\n",
    "plt.show()"
   ]
  },
