   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "a, b = 16, 14\n",
    "radii = [4.25, 6, 8]\n",
    "\n",
    "x_fill = np.linspace(2, 13, 4000)\n",
    "\n",
    "inside = 49 - (x_fill - 8)**2\n",
    "y_low  = 9 - np.sqrt(inside)\n",
    "y_high = 9 + np.sqrt(inside)\n",
    "\n",
    "y_line = 24 - x_fill\n",
    "\n",
    "y_top = np.minimum(y_high, y_line)\n",
    "\n",
    "mask = (inside >= 0) & (y_top >= y_low)\n",
    "\n",
    "# ---- Plot boundaries ----\n",
    "t = np.linspace(0, 2*np.pi, 1200)\n",
    "xc = 8 + 7*np.cos(t)\n",
    "yc = 9 + 7*np.sin(t)\n",
    "\n",
    "x_line = np.linspace(0, 25, 600)\n",
    "y_line_plot = 24 - x_line\n",
    "\n",
    "plt.figure(figsize=(8, 7))\n",
    "\n",
    "plt.plot(xc, yc, color=\"C0\",label=r'$(x-8)^2+(y-9)^2\\leq 49$')\n",
    "plt.plot(x_line, y_line_plot, color=\"C1\",label=r'$x+y\\leq 24$')\n",
    "plt.axvline(2, color=\"C2\", linestyle=\"--\", label=r'$x\\geq2$')\n",
    "plt.axvline(13, color=\"C2\",linestyle=\"--\", label=r'$x\\leq13$')\n",
    "\n",
    "plt.fill_between(x_fill, y_low, y_top, where=mask, color=\"violet\", alpha=0.35, interpolate=True)\n",
    "\n",
    "plt.scatter([a], [b], marker=\"x\", s=100, label=f'Objective center ({a},{b})')\n",
    "\n",
    "for i, r in enumerate(radii):\n",
    "    xo = a + r*np.cos(t)\n",
    "    yo = b + r*np.sin(t)\n",
    "    plt.plot(xo, yo, color=\"C3\",linestyle=\"--\", linewidth=2)\n",
    "\n",
    "\n",
    "plt.title(\"Example 7\")\n",
    "plt.xlabel(r\"$x$\",fontsize=14)\n",
    "plt.ylabel(r\"$y$\",fontsize=14)\n",
    "plt.grid(True)\n",
    "plt.gca().set_aspect(\"equal\", adjustable=\"box\")\n",
    "\n",
    "plt.xlim(0, 25)\n",
    "plt.ylim(0, 25)\n",
    "\n",
    "plt.legend(loc=\"best\")\n",
    "plt.tight_layout()\n",
    "plt.savefig(\"Example7.png\", dpi=600, bbox_inches=\"tight\", facecolor=\"white\")\n",
    "plt.show()\n"
   ]
  },
