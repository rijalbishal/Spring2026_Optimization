   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "f = lambda x: 3*x**4 - 4*x**3 + 1\n",
    "x = np.linspace(-0.5, 1.5, 2000)\n",
    "\n",
    "xh, yh = 0.0, 1.0\n",
    "xi, yi = 2/3, f(2/3)  \n",
    "\n",
    "plt.figure(figsize=(7,4.5))\n",
    "plt.plot(x, f(x), lw=2, label=r\"$f(x)=3x^4-4x^3+1$\")\n",
    "\n",
    "plt.scatter([xh], [yh], s=70, zorder=3)\n",
    "plt.scatter([xi], [yi], s=70, marker=\"x\", zorder=3)\n",
    "\n",
    "plt.annotate(\"Horizontal inflection\\n(0, 1)\",\n",
    "             xy=(xh, yh), xytext=(20, 35), textcoords=\"offset points\",\n",
    "             arrowprops=dict(arrowstyle=\"->\"))\n",
    "\n",
    "plt.annotate(\"Other inflection\\n(2/3, 11/27)\",\n",
    "             xy=(xi, yi), xytext=(50, 10), textcoords=\"offset points\",\n",
    "             arrowprops=dict(arrowstyle=\"->\", connectionstyle=\"arc3,rad=0.25\"))\n",
    "\n",
    "plt.grid(True)\n",
    "plt.xlabel(\"x\", fontsize=14)\n",
    "plt.ylabel(\"f(x)\", fontsize=14)\n",
    "plt.legend(loc=\"best\")\n",
    "plt.tight_layout()\n",
    "plt.savefig(\"Point_of_inflection.png\", dpi=600, bbox_inches=\"tight\", facecolor=\"white\")\n",
    "plt.show()\n"
   ]
  }
